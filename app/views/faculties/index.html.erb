<p id="notice"><%= notice %></p>

<h1>Faculties</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Department</th>
      <th>Qualification</th>
      <th>designation</th>
       <th>Experience</th>
      <th>Specialization</th>
      <th>Contact</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @faculties.each do |faculty| %>
      <tr>
        <td><%= faculty.name %></td>
        <td><%= faculty.department %></td>
        <td><%= faculty.qualification %></td>
        <td><%= faculty.designation %></td>
        <td><%= faculty.Experience %></td>
        <td><%= faculty.specialization %></td>
        <td><%= faculty.contact %></td>
        <td><%= faculty.email %></td>
        <td><%= link_to 'Show', faculty %></td>
        <td><%= link_to 'Edit', edit_faculty_path(faculty) %></td>
        <td><%= link_to 'Destroy', faculty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Faculty', new_faculty_path %>
